<?xml version="1.0" encoding="utf-8"?> 
<AutoVisualizer xmlns="http://schemas.microsoft.com/vstudio/debugger/natvis/2010">

  <Type Name="AutoArray&lt;*,*&gt;">
    <DisplayString>{{size = {mpLast - mpArray}}}</DisplayString>
    <Expand>
      <ArrayItems>
        <Size>mpLast - mpArray</Size>
        <ValuePointer>mpArray</ValuePointer>
      </ArrayItems>
      <Item Name="capacity">mpEnd - mpArray</Item>
    </Expand>
  </Type>
  
</AutoVisualizer>